{
  "name": "gigclaw-agents",
  "version": "0.1.0",
  "description": "Agent workers for GigClaw marketplace",
  "main": "dist/coordinator.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/coordinator.ts",
    "build": "tsc",
